                "                        'Year': 'first',\n",
                "                        'Install.Type': 'first',\n",
                "                        'Borough': 'first',\n",
                "                        'ntacode': 'first'\n",
                "                    }\n",
                "daily_avg_temps = temps.groupby(['Sensor.ID', 'Day']).agg(aggregation_functions).reset_index()\n",
                "\n",
