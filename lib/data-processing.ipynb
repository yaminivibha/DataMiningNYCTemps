            "source": [
                "# Sampling the temp data\n",
                "# Keeping only hour 0, 6, 12, 18 for each day for each location\n",
                "# daily_avg_temps = temps[temps['Hour'].isin([0, 6, 12, 18])]\n",
                "aggregation_functions = {\n",
